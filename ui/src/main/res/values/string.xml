    <string name="bca_klik_pay_title">BCA KlikPay</string>
    <string name="bca_klik_pay_instruction">Please complete BCA KlikPay through BCA KlikPay app/website.</string>
    <string name="bca_klik_pay_how_to_pay_title">How to pay</string>
    <string-array name="bca_klik_pay_how_to_pay">
        <item><data><![CDATA[Tap <b>Pay Now</b> and you will be redirected to BCA KlikPay page.]]></data></item>
        <item><data><![CDATA[Login and check your payment details.]]></data></item>
        <item><data><![CDATA[Choose KlikBCA or BCA Card to pay for your transaction.]]></data></item>
        <item><data><![CDATA[To authorize payment with BCA KlikPay, tap the <b>Send OTP</b> button to receive an OTP code via SMS, then enter code in the fields provided.]]></data></item>
        <item><data><![CDATA[If your OTP code is correct, your payment will be processed immediately.]]></data></item>
        <item><data><![CDATA[Once the payment is completed, success status will appear on the transaction screen and you will receive a notification e-mail.]]></data></item>
    </string-array>
    <string name="bca_klik_pay_cta">Pay now</string>


